{
  "name": "mentha-web-archimate-client",
  "version": "1.0.0",
  "description": "Mentha Archimate",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run build:webpack:prod",
    "build:webpack:prod": "webpack --config src/main/webpack/webpack.prod.config.js",
    "build:webpack:dev": "webpack --config src/main/webpack/webpack.dev.config.js",
    "dev": "node src/main/webpack/webpack.server.dev.js",
    "lint": "cross-env-shell eslint src --ext .jsx --ext .js",
    "test": "npm run lint"
  },
  "babel": {
    "presets": [
      "es2017",
      "es2016",
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-react-remove-prop-types",
      "transform-react-constant-elements",
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "redux-undo": "^0.6.1",
    "material-ui": "^1.0.0-beta.40",
    "material-ui-icons": "^1.0.0-beta.36",
    "lodash": "^4.17.5",
    "svg-intersections": "^0.4.0",
    "react-localstorage": "^0.3.1",
    "typeface-roboto": "^0.0.54",
    "dom-to-image": "^2.6.0",
    "file-saver": "^1.3.8",
    "qs": "^6.5.1",
    "qhistory": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "react-hot-loader": "^4.0.0",
    "colors": "^1.2.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "ignore-styles": "^5.0.1",
    "mocha": "^5.0.5",
    "node-sass": "^4.8.3",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.2",
    "http-proxy-middleware": "^0.18.0",
    "webpack-merge": "^4.1.2",
    "detect-port": "^1.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "whatwg-fetch": "^2.0.3",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3"
  }
}
