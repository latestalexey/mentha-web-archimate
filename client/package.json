{
  "name": "mentha-web-archimate-client",
  "version": "1.0.0",
  "description": "Mentha Archimate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --history-api-fallback --host localhost --port 8080",
    "build-dev": "webpack --config webpack.dev.config.js",
    "build-prod": "webpack -p --config webpack.prod.config.js",
    "test": "mocha --recursive --compilers js:babel-register --require babel-polyfill --require ignore-styles",
    "test-watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "es2017",
      "es2016",
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy",
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "license": "MIT",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-dnd": "^2.5.3",
    "react-dnd-html5-backend": "^2.5.3",
    "react-tap-event-plugin": "^2.0.1",
    "material-ui": "^1.0.0-beta.13",
    "material-ui-icons": "^1.0.0-beta.14",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0",
    "redux-undo": "^0.6.1",
    "lodash": "^4.17.4",
    "svg-intersections": "^0.3.1",
    "react-localstorage": "^0.3.1",
    "typeface-roboto": "^0.0.35",
    "dom-to-image": "^2.5.2",
    "file-saver": "^1.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "mocha": "^4.0.0",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^1.3.1",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "whatwg-fetch": "^2.0.3"
  }
}
